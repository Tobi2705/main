    //- .advertiser
    //-   p Hire the best tech players for your company 
    //-   a(href='https://toomai.typeform.com/to/VnpRAv' target='_blank').button Post a Job

    each post in paid
      table.joblist#paidjobs
          tr(data-type=`${post['tags']}`, data-location=`${post['location']}`)
            td
              p.position= post['position'] 
              p.company= post['company']
            td
              div.tags  
               ul#tagslist
                p(data-type=`${post['tags']}`)#tags= post['tags']
            td
              p.location(data-type=`${post['location']}`)= post['location']
            td
              p#date #{moment(post['date']).format("MMMM Do")}
              
            td 
              a(href='https://sportekjobs.com/' + post['slug']).button.info Get more Info 
    .advertiser
      #revue-embed
              form#revue-form(action='https://www.getrevue.co/profile/tobias-hikari/add_subscriber', method='post', name='revue-form', target='_blank')
                .revue-form-group
                  label(for='member_email') 
                  input#member_email.field(placeholder='Your email address...', type='email', name='member[email]')
                
                .revue-form-actions
                  input#subscribe.button(type='submit', value='Subscribe to Newsletter', name='member[subscribe]')
    h2 Recent Jobs
    //-  img(src='data:image/png;base64,')
    

    each post in data
      table.joblist.all-jobs#joblist
          tr(data-type=`${post['tags']}`, data-location=`${post['location']}`)
            td
              p.position= post['position'] 
              p.company= post['company']
            td
              div.tags  
               ul#tagslist
                p(data-type=`${post['tags']}`)#tags= post['tags']
            td
              p.location(data-type=`${post['location']}`)= post['location']
            td
              p#date #{moment(post['date']).startOf('day').fromNow()}
            td 
              a(href='https://sportekjobs.com/' + post['slug']).button.info Get more Info 
    table.joblist#remote-jobs